<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="pen-proj_tablewithbredcrumb.TABLEWITHBREDCRUMB" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Table with Breadcrumbs ">
		<content>
	
					<Breadcrumbs currentLocationText="Laptop">
				<Link press="onPress" text="Years"/>
				<Link press="onPress" text="months"/>
				
			</Breadcrumbs>
						<Table
				selectionChange="handleSelectionChange"
				inset="false"
				id="idProductsTable">
				<headerToolbar>
					<Toolbar
						id="idCrumbToolbar" />
				</headerToolbar>
				<infoToolbar>
					<Toolbar>
						<Label/>
					</Toolbar>
				</infoToolbar>
				<columns>
					<Column
						width="12em">
						<Text text="Years" />
					</Column>
					
					
				</columns>
				<items>
				<ColumnListItem>
				<Button text="2017" press="Onpress1"/>
				</ColumnListItem>
				<ColumnListItem>
				<Button text="2016"/>
				</ColumnListItem>
				<ColumnListItem>
				<Button text="2015"/>
				</ColumnListItem>
				<ColumnListItem>
				<Button text="2014"/>
				</ColumnListItem>
				</items>
			</Table>
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button text="Previous" press="handleOrderPress" />
			</Toolbar>
		</footer>
	</Page>
			
	
		
</core:View>