<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" 
		controllerName="com.smax.empreg.routing.controller.RegistrationDetailsPage" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Registration Details Page" showHeader="true">
		<content>
			<f:SimpleForm id="empRegDetailsForm"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Employee Personal Details"
			labelSpanXL="3"
			labelSpanL="4"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" 
			backgroundDesign="Solid">
			<f:content>
				<Label text="Employee Name" />
				<Text text="{Name}" />
				<Label text="Department" />
				<Text text="{dept}" />
				<Label text="Role" />
				<Text text="{role}" />
				<Label text="Email" />
				<Text text="{email}" />
				<Label text="PAN No." />
				<Text text="{panno}" />
				<Label text="Country." />
				<Text text="{country}" />
			</f:content>
		</f:SimpleForm>	
			
		</content>
	</Page>
</core:View>